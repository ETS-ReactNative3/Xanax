{
  "name": "kdps/xanax",
  "type": "project",
  "description": "PHP Framework or Libraries",
  "keywords": ["php framework", "php library"],
  "license": "MIT",
  "require": {
    "php": ">=7.0.0",
    "phpmailer/phpmailer": "^6.1",
    "ezyang/htmlpurifier": ">=4.10.0"
  },
  "autoload": {
    "psr-4": {"Xanax\\": "Classes/Src/"},
    "classmap": [
      ""
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
