{
	"name": "kdps/Xanax",
	"type": "project",
	"description": "PHP Framework or Libraries",
	"keywords": ["php framework", "php library"],
	"license": "MIT",
	"require": {
		"ext-sockets": "*",
		"php": ">=7.0"
	},
	"autoload": {
		"psr-4": {"Xanax\\Raw\\": "Classes/Src"},
        "classmap": [
            ""
        ]
	},
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
}
